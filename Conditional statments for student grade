#include <stdio.h>
int main()
{
int a;
printf("Enter your marks",&a);
Scanf("%d",&a);
{
if(85<a<100)
printf("Grade A",a);
scanf("%d",&a);
}
{
if(70<a<84)
printf("Grade B",a);
scanf("%d",&a);
}
{
if(55<a<69)
printf("Grade C",a);
scanf("%d%",&a);
}
{
if(40<a<54)
printf("Grade D",a);
scanf("%d%,&a);
}
{
if(a<40)
printf("Grade F",a);
scanf("%d",&a);
}
Return 0;
}
}
